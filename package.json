{
  "name": "todo-api-ts-ed",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "pnpm run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "pnpm run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "pnpm run test:lint && pnpm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "pnpm run test:unit",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "ts-node ./src/seed/index.ts"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@prisma/client": "^5.0.0",
    "@tsed/ajv": "^7.34.2",
    "@tsed/cli-core": "4.6.4",
    "@tsed/common": "^7.34.2",
    "@tsed/core": "^7.34.2",
    "@tsed/di": "^7.34.2",
    "@tsed/engines": "^7.34.2",
    "@tsed/exceptions": "^7.34.2",
    "@tsed/json-mapper": "^7.34.2",
    "@tsed/logger": "^6.6.2",
    "@tsed/logger-file": "^6.6.2",
    "@tsed/openspec": "^7.34.2",
    "@tsed/platform-cache": "^7.34.2",
    "@tsed/platform-exceptions": "^7.34.2",
    "@tsed/platform-koa": "^7.34.2",
    "@tsed/platform-log-middleware": "^7.34.2",
    "@tsed/platform-middlewares": "^7.34.2",
    "@tsed/platform-params": "^7.34.2",
    "@tsed/platform-response-filter": "^7.34.2",
    "@tsed/platform-views": "^7.34.2",
    "@tsed/prisma": "^7.34.2",
    "@tsed/schema": "^7.34.2",
    "@tsed/swagger": "^7.34.2",
    "ajv": "^8.12.0",
    "barrelsby": "^2.8.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-compress": "^5.1.1",
    "koa-override": "^3.0.0",
    "koa-qs": "^3.0.0"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.6.4",
    "@tsed/cli-plugin-jest": "4.6.4",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.3",
    "@types/koa": "^2.13.8",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-json": "^2.0.20",
    "@types/koa-qs": "^2.0.0",
    "@types/koa-send": "^4.1.3",
    "@types/koa__cors": "^4.0.0",
    "@types/koa__router": "^12.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  },
  "tsed": {
    "convention": "conv_default",
    "architecture": "arc_default",
    "packageManager": "pnpm"
  }
}
